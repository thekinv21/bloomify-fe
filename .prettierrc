{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/hooks",
		"^@/config",
		"^@/constant",
		"^@/types",
		"^@/services",
		"^@/provider",
		"^@/screens",
		"^@/components",
		"^@/store",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$",
		"(.css)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true,
	"importOrderGroupNamespaceSpecifiers": true,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports",
		"prettier-plugin-tailwindcss"
	]
}
